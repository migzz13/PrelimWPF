        <Button x:Name="RuleBtn" Content="How to play" BorderThickness="3" Margin="0,0,164,31" Background="#FFFF793B" BorderBrush="White" Foreground="White" FontSize="30" FontFamily="Lucida Console" Click="RuleBtn_Click" Height="51" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="237" Focusable="False"/>
        <Button x:Name="Switch1" Content="Switch" BorderThickness="3" Margin="81,0,0,126" BorderBrush="Black" FontFamily="Lucida Console" FontSize="14" Height="36" VerticalAlignment="Bottom" HorizontalAlignment="Left" Width="64" Foreground="Black" Background="White" Click="Switch1_Click"/>
